{
  "name": "rehype-pretty-code",
  "version": "0.12.1",
  "description": "Beautiful code for your MD/MDX docs.",
  "main": "./dist/rehype-pretty-code.js",
  "module": "./dist/rehype-pretty-code.js",
  "type": "module",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/rehype-pretty-code.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npx rollup -c",
    "test": "vitest",
    "lint": "eslint . --ext .js,.ts,.json",
    "typecheck": "tsc --noEmit",
    "playground": "npm run dev --workspace=word-highlighter-playground",
    "playground-website": "npm run dev --workspace=website",
    "release": "bumpp package.json --commit --push --tag",
    "prebuild": "rm -rf dist",
    "prepublishOnly": "NODE_ENV='production' npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "node 18"
  ],
  "prettier": {
    "singleQuote": true,
    "proseWrap": "always"
  },
  "author": "",
  "license": "MIT",
  "repository": "github:atomiks/rehype-pretty-code",
  "homepage": "https://rehype-pretty-code.netlify.app",
  "peerDependencies": {
    "shikiji": "^0.7.0 || ^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.43",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hast-util-to-html": "^8.0.3",
    "jest-file-snapshot": "^0.5.0",
    "mdast-util-to-hast": "^12.1.0",
    "prettier": "^3.1.0",
    "remark": "^14.0.2",
    "rollup": "^4.6.1",
    "shikiji": "^0.7.6",
    "shikiji-transformers": "^0.7.6",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@types/hast": "^3.0.3",
    "hast-util-to-string": "^3.0.0",
    "parse-numeric-range": "^1.3.0",
    "rehype-parse": "^9.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  }
}
